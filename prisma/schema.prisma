// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

enum EGameType {
    game
    dlc
}

model Game {
    id        String    @id @default(uuid())
    steam_id  Int       @unique
    steam_url String
    title     String
    image     String
    price     Int
    game_type EGameType
}
